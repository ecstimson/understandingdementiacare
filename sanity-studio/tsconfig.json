{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "esnext"],
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "noEmit": true
  }
}

